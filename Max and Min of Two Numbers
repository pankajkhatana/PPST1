#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Enter two numbers: ";
    cin >> a >> b;

    if (a > b) {
        cout << "Max = " << a << endl;
        cout << "Min = " << b << endl;
    } else {
        cout << "Max = " << b << endl;
        cout << "Min = " << a << endl;
    }

    return 0;
}
